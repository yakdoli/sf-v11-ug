    <splits>
      <split>@&quot;</split>
      <split>(*</split>
      <split>*)</split>
      <split>//</split>
      <split>'</split>
      <split>///</split>
      <split>#Region</split> 
      <split>#End Region</split>
      <split>End If</split>
      <split>Else If</split>
      <split>End Sub</split>
      <split>End Function</split>
